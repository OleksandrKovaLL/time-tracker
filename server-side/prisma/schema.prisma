generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model TimeEntry {
  id          String   @id @default(uuid())
  date        DateTime
  project     String
  hours       Float
  description String
  createdAt   DateTime @default(now())

  @@map("time_entries")
}
